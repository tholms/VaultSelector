{
	"name": "obsidian-vault-selector",
	"version": "1.0.0",
	"description": "A plugin to manage vault selection in Obsidian",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": ["obsidian", "plugin", "vault"],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@eslint/js": "^8.56.0",
		"eslint": "^8.56.0",
		"globals": "^14.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.5",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
